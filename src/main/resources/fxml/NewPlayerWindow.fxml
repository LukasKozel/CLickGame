<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cz.my.learn.clickgame.controllers.NewPlayerWindowController">
   <children>
      <VBox alignment="CENTER" layoutX="75.0" layoutY="25.0" prefHeight="200.0" prefWidth="100.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
         <children>
            <Label text="Write your name">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="wrongNameLabel" />
            <TextField fx:id="newNameTextField" />
            <Button fx:id="okButton" mnemonicParsing="false" onAction="#okButtonAction" prefWidth="50.0" text="OK" />
         </children>
      </VBox>
   </children>
</AnchorPane>
